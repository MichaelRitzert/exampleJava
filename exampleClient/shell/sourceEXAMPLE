if [ -v EPICSV4 ]
then
    echo "EPICSV4 already defined as " + $EPICSV4
else
    export EPICSV4=/home/epicsv4/master
fi
export PVDATA=${EPICSV4}/pvDataJava
export PVACCESS=${EPICSV4}/pvAccessJava
export PVACLIENT=${EPICSV4}/pvaClientJava
export NORMATIVETYPES=${EPICSV4}/normativeTypesJava
export EXAMPLECLIENT=${EPICSV4}/exampleJava/exampleClient
export CLASSPATH=${PVDATA}/target/classes
export CLASSPATH=${CLASSPATH}:${PVACCESS}/target/classes
export CLASSPATH=${CLASSPATH}:${PVACLIENT}/target/classes
export CLASSPATH=${CLASSPATH}:${NORMATIVETYPES}/target/classes
export CLASSPATH=${CLASSPATH}:${EXAMPLECLIENT}/target/classes
export CLASSPATH=${CLASSPATH}:${EXAMPLECLIENT}/shell/caj-1.1.15.jar
export CLASSPATH=${CLASSPATH}:${EXAMPLECLIENT}/shell/jca-2.3.6.jar

